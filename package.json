{
  "name": "zz-ui",
  "version": "1.0.0",
  "description": "A Component Library For Vue.js",
  "author": "469547164@qq.com",
  "repository": {
    "type": "git",
    "url": "git@github.com:zuopf769/ZZ-UI.git"
  },
  "license": "MIT",
  "main": "lib/zz-ui.common.js",
  "files": [
    "lib",
    "src",
    "components",
    "types",
    "helper",
    "components.json"
  ],
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.demo.conf.js",
    "start": "npm run dev",
    "copy:types": "cp -r types/*.d.ts lib",
    "build": "node build/build.js",
    "clean": "rimraf lib && rimraf dist && rimraf components/*/lib && rimraf test/**/coverage && rimraf examples/zz-ui",
    "build:dist": "npm run clean && npm run build:entry && npm run build:component && npm run build:common && npm run build:utils && npm run build:index && npm run build:css && npm run build:theme && npm run build:types && npm run copy:types",
    "build:entry": "node build/bin/build-entry.js",
    "build:types": "node build/bin/build-ts.js",
    "build:utils": "cross-env BABEL_ENV=utils babel src --out-dir lib --ignore src/index.js",
    "build:index": "webpack --config build/webpack.umd.conf.js",
    "build:common": "webpack --config build/webpack.common.conf.js",
    "build:component": "webpack --config build/webpack.component.config.js",
    "build:css": "node build/bin/build-css.js",
    "build:theme": "gulp --gulpfile gulpfile.js build",
    "build:theme-chalk": "npm run build:css && gulp --gulpfile gulpfile.js build:theme-chalk",
    "build:theme2": "npm run build:css && gulp --gulpfile gulpfile.js build:theme2",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.vue\"",
    "lint": "eslint --ext .js,.ts,.vue src components examples",
    "lint:fix": "npm run lint -- --fix",
    "commit": "git add -A && npx git-cz",
    "changelog": "npx conventional-changelog -p angular -i CHANGELOG.md -s -p && git add CHANGELOG.md"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.9.2",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "vue-property-decorator": "^8.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/highlight.js": "^9.12.4",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "@typescript-eslint/typescript-estree": "^2.27.0",
    "autoprefixer": "^9.7.5",
    "babel-loader": "^8.1.0",
    "chalk": "^2.0.1",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.31",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.1.0",
    "highlight.js": "^9.18.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.3",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.2.7",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^2.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-notifier": "^5.1.2",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "^1.2.0",
    "portfinder": "^1.0.13",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "prettier": "^2.0.4",
    "resolve-url-loader": "^3.1.1",
    "rimraf": "^2.6.0",
    "sass-loader": "^8.0.2",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^0.5.8",
    "vue": "^2.6.11",
    "vue-highlight.js": "^3.1.0",
    "vue-loader": "^15.9.1",
    "vue-markdown-loader": "^2.4.1",
    "vue-router": "^3.1.6",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "peerDependencies": {
    "vue": "^2.5.17"
  },
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 5.0.0"
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,vue}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
